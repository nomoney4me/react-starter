{
  "name": "react-starter",
  "version": "2.1.1",
  "description": "Basic Webpack usage",
  "main": "src/main.js",
  "scripts": {
    "start": "webpack-dev-server -d --open --progress --hot --inline",
    "dev": "webpack -d --watch --progress"
  },
  "keywords": [
    "webpack",
    "javascript",
    "modules"
  ],
  "author": "Tien Bui",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "dot-env": "0.0.1",
    "ify-loader": "^1.1.0",
    "jquery": "^3.3.1",
    "loading-svg": "^1.0.0",
    "match-sorter": "^2.2.0",
    "orb": "^1.0.9",
    "plotly.js": "^1.33.1",
    "popper.js": "^1.12.9",
    "react-bootstrap-table": "^4.3.1",
    "react-csv": "^1.0.12",
    "react-filtered-multiselect": "^0.5.1",
    "react-pivot": "^3.0.0",
    "react-pivottable": "^0.3.1",
    "react-plotly.js": "^1.3.0",
    "react-table": "^6.7.6",
    "react-widgets": "^4.1.2",
    "striptags": "^3.1.1"
  }
}
